{
  "name": "champaignlocal",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "moment": "^2.29.1",
    "nouislider": "^14.6.2",
    "react": "^17.0.1",
    "react-bootstrap-switch": "^15.5.3",
    "react-datetime": "2.16.3",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "reactstrap": "^8.7.1",
    "sass": "^1.29.0"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
